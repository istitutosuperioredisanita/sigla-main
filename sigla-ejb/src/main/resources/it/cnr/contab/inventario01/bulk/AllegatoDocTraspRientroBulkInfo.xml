<bulkInfo
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://contab.cnr.it/schema/SIGLA"
        xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
    <fieldProperty
            name="aspectName"
            property="aspectName"
            inputType="SELECT"
            keysProperty="aspectNamesKeys"
            command="doDefault"
            inputSize="40"
            nullable="false"
            label="Tipologia"/>
    <fieldProperty
            name="utente"
            inputType="TEXT"
            property="utenteSIGLA"
            label="Utente"/>
    <fieldProperty
            name="data"
            inputType="TEXT"
            property="lastModificationDate"
            formatName="timestamp"
            label="Data"/>
    <formFieldProperty
            name="aspectName" />
    <columnFieldProperty
            name="aspectName" />
    <columnSet
            name="default" >
        <columnFieldProperty
                name="aspectName" />
        <columnFieldProperty
                name="nome" />
        <columnFieldProperty
                name="descrizione" />
        <columnFieldProperty
                name="utente"/>
        <columnFieldProperty
                name="data"/>
    </columnSet>
    <form
            name="default" >
        <formFieldProperty
                name="aspectName" />
        <formFieldProperty
                name="nome" />
        <formFieldProperty
                name="descrizione" />
        <formFieldProperty
                name="file" />
        <formFieldProperty
                name="attivaFile" />
    </form>

</bulkInfo>
