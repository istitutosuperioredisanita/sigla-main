<SQLPersistentInfo
		persistentClassName="it.cnr.contab.inventario01.bulk.Doc_trasporto_rientroBulk"
		homeClassName="it.cnr.contab.inventario01.bulk.Doc_trasporto_rientroHome">

	<defaultColumnMap tableName="DOC_TRASPORTO_RIENTRO">

		<columnMapping
				columnName="PG_INVENTARIO"
				propertyName="inventario.pg_inventario"
				sqlTypeName="DECIMAL"
				columnSize="38" />

		<columnMapping
				columnName="CD_TIPO_TRASPORTO_RIENTRO"
				propertyName="tipoMovimento.cdTipoTrasportoRientro"
				sqlTypeName="VARCHAR"
				columnSize="10" />

		<columnMapping
				columnName="CD_TERZO_ASSEGNATARIO"
				propertyName="terzoIncRitiro.cd_terzo"
				sqlTypeName="DECIMAL"
				columnSize="38" />

		<columnMapping
				columnName="CD_TERZO_RESPONSABILE"
				propertyName="terzoRespDip.cd_terzo"
				sqlTypeName="DECIMAL"
				columnSize="38" />

		<columnMapping columnName="TI_DOCUMENTO_RIF" propertyName="tiDocumentoRif" sqlTypeName="CHAR" columnSize="1"/>
		<columnMapping columnName="ESERCIZIO_RIF" propertyName="esercizioRif" sqlTypeName="DECIMAL" columnSize="5"/>
		<columnMapping columnName="PG_DOC_TRASPORTO_RIENTRO_RIF" propertyName="pgDocTrasportoRientroRif" sqlTypeName="DECIMAL" columnSize="10"/>


	</defaultColumnMap>

	<!-- FK che fa parte della PK -->
	<persistentProperty name="inventario" partOfOid="true" />

	<!-- FK che NON fanno parte della PK -->
	<persistentProperty name="tipoMovimento" partOfOid="false" />
	<persistentProperty name="terzoIncRitiro" partOfOid="false" />
	<persistentProperty name="terzoRespDip" partOfOid="false" />
	<!-- Nuove proprietÃ  -->
	<persistentProperty name="tiDocumentoRif"/>
	<persistentProperty name="esercizioRif"/>
	<persistentProperty name="pgDocTrasportoRientroRif"/>

</SQLPersistentInfo>