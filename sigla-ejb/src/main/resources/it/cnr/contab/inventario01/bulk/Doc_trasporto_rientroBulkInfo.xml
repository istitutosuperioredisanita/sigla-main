<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA"
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">

	<!-- ============================================ -->
	<!-- CAMPI BASE DOCUMENTO                         -->
	<!-- ============================================ -->

	<fieldProperty
			name="pg_inventario"
			property="inventario.pg_inventario"
			inputType="ROTEXT"
			maxLength="10"
			inputSize="10"
			label="Inventario" />

	<fieldProperty
			name="ti_documento"
			property="ti_documento"
			maxLength="1"
			inputSize="1"
			inputType="ROTEXT"
			label="Tipo Documento" />

	<fieldProperty
			name="esercizio"
			property="esercizio"
			inputType="TEXT"
			inputSize="4"
			maxLength="4"
			enabledOnFreeSearch="false"
			enabledOnInsert="false"
			enabledOnEdit="false"
			enabledOnSearch="false"
			nullable="false"
			label="Esercizio" />

	<fieldProperty
			name="pg_doc_trasporto_rientro"
			property="pg_doc_trasporto_rientro"
			inputType="TEXT"
			inputSize="10"
			maxLength="38"
			enabledOnFreeSearch="true"
			enabledOnInsert="false"
			enabledOnEdit="false"
			enabledOnSearch="true"
			nullable="false"
			label="Numero Documento" />

	<fieldProperty
			name="ds_doc_trasporto_rientro"
			property="ds_doc_trasporto_rientro"
			inputType="TEXT"
			maxLength="100"
			inputSize="100"
			nullable="false"
			caseSensitiveSearch="false"
			label="Descrizione"/>

	<fieldProperty
			name="data_registrazione"
			property="data_registrazione"
			inputType="TEXT"
			maxLength="10"
			inputSize="10"
			nullable="false"
			formatName="date_short"
			label="Data Registrazione"
			command="doOnData_registrazioneChange"/>

	<fieldProperty
			name="cd_tipo_trasporto_rientro"
			property="tipoMovimento.cd_tipo_trasporto_rientro"
			inputType="TEXT"
			inputSize="10"
			maxLength="10"
			nullable="false"
			label="Codice Tipo Movimento" />

	<fieldProperty
			name="tipoMovimento"
			property="tipoMovimento"
			printProperty="ds_tipo_trasporto_rientro"
			optionsProperty="tipoMovimenti"
			maxLength="100"
			inputSize="100"
			inputType="SELECT"
			nullable="false"
			enabledOnSearch="true"
			label="Tipo Movimento"/>

	<fieldProperty
			name="stato"
			property="stato"
			inputType="SELECT"
			maxLength="3"
			inputSize="3"
			nullable="false"
			enabledOnSearch="true"
			enabledOnEdit="true"
			label="Stato"/>

	<fieldProperty
			name="destinazione"
			property="destinazione"
			inputType="TEXT"
			maxLength="100"
			inputSize="100"
			caseSensitiveSearch="false"
			label="Destinazione"/>

	<fieldProperty
			name="indirizzo"
			property="indirizzo"
			inputType="TEXTAREA"
			cols="60"
			rows="3"
			maxLength="200"
			label="Indirizzo"/>

	<!-- ============================================ -->
	<!--SEZIONE RITIRO DELEGATO               -->
	<!-- ============================================ -->

	<fieldProperty
			name="fl_incaricato"
			property="fl_incaricato"
			inputType="CHECKBOX"
			nullable="false"
			label="Ritiro Incaricato"
			command="doOnFlIncaricatoChange"/>

	<fieldProperty
			name="fl_vettore"
			property="fl_vettore"
			inputType="CHECKBOX"
			nullable="false"
			label="Ritiro Vettore"
			command="doOnFlVettoreChange"/>

	<!--CAMPO DIPENDENTE (CONDIZIONALE)-->
	<!-- Visibile SOLO se fl_incaricato = Y o fl_vettore = Y -->
	<fieldProperty
			name="find_dipendente"
			property="dipendente"
			inputType="SEARCHTOOL"
			formName="searchtool"
			label="Dipendente (Incaricato/Vettore)"
			enabledOnInsert="true"
			enabledOnEdit="true"
			enabledOnSearch="false"
			command="doOnDipendenteChange"/>

	<fieldProperty
			name="cd_dipendente"
			property="dipendente.cd_anagr"
			inputType="TEXT"
			maxLength="20"
			inputSize="20"
			label="Codice Dipendente"
			caseSensitiveSearch="false" />

	<fieldProperty
			name="denominazione_dipendente"
			property="dipendente.denominazione_sede"
			inputType="TEXT"
			maxLength="100"
			inputSize="50"
			label="Nome Dipendente"
			caseSensitiveSearch="false" />

	<fieldProperty
			name="uo_dipendente"
			property="dipendente.cd_unita_organizzativa"
			inputType="ROTEXT"
			maxLength="30"
			inputSize="30"
			label="UO Dipendente" />

	<!--NOTE RITIRO (CONDIZIONALI)-->
	<!-- Abilitate SOLO se fl_incaricato = Y o fl_vettore = Y -->
	<fieldProperty
			name="note_ritiro"
			property="note_ritiro"
			inputType="TEXTAREA"
			cols="60"
			rows="5"
			maxLength="4000"
			label="Note Ritiro"/>

	<fieldProperty
			name="note"
			property="note"
			inputType="TEXTAREA"
			cols="60"
			rows="5"
			maxLength="4000"
			label="Note Motivo Trasporto"/>

	<!-- ============================================ -->
	<!-- RICERCA BENI                                 -->
	<!-- ============================================ -->

	<fieldProperty
			name="nr_inventario"
			property="nr_inventario"
			maxLength="10"
			inputSize="10"
			inputType="TEXT"
			label="Codice Bene"/>

	<fieldProperty
			name="cd_barre"
			property="cd_barre"
			maxLength="6"
			inputSize="6"
			inputType="TEXT"
			label="Cod. barre"/>

	<!-- ============================================ -->
	<!-- DATI CONSEGNATARIO                          -->
	<!-- ============================================ -->

	<fieldProperty
			name="cd_consegnatario"
			property="consegnatario.cd_terzo"
			inputType="ROTEXT"
			label="Consegnatario"
			maxLength="6"
			inputSize="6"
			enabledOnSearch="true"
			enabledOnEdit="true"
			caseSensitiveSearch="false" />

	<fieldProperty
			name="cognome_consegnatario"
			property="consegnatario.denominazione_sede"
			inputType="ROTEXT"
			maxLength="30"
			inputSize="30"
			enabledOnSearch="true"
			enabledOnEdit="true"
			caseSensitiveSearch="false" />

	<!-- ============================================ -->
	<!-- DATI DELEGATO                               -->
	<!-- ============================================ -->

	<fieldProperty
			name="cd_delegato"
			property="delegato.cd_terzo"
			inputType="ROTEXT"
			label="Delegato"
			maxLength="6"
			inputSize="6"
			enabledOnSearch="true"
			enabledOnEdit="true"
			caseSensitiveSearch="false" />

	<fieldProperty
			name="cognome_delegato"
			property="delegato.denominazione_sede"
			inputType="ROTEXT"
			maxLength="30"
			inputSize="30"
			enabledOnSearch="true"
			enabledOnEdit="true"
			caseSensitiveSearch="false" />

	<!-- ============================================ -->
	<!-- DATI UO CONSEGNATARIA                       -->
	<!-- ============================================ -->

	<fieldProperty
			name="cd_uo_consegnataria"
			property="uo_consegnataria.cd_unita_organizzativa"
			inputType="ROTEXT"
			label="UO Consegnataria"
			maxLength="6"
			inputSize="6"
			enabledOnSearch="true"
			enabledOnEdit="true"
			caseSensitiveSearch="false" />

	<fieldProperty
			name="ds_uo_consegnataria"
			property="uo_consegnataria.ds_unita_organizzativa"
			inputType="ROTEXT"
			maxLength="30"
			inputSize="30"
			enabledOnSearch="true"
			enabledOnEdit="true"
			caseSensitiveSearch="false" />

	<!-- ============================================ -->
	<!-- FORM FIELDS PROPERTY                         -->
	<!-- ============================================ -->

	<formFieldProperty name="tipoMovimento" />
	<formFieldProperty name="cd_uo_consegnataria" />
	<formFieldProperty name="ds_uo_consegnataria" />
	<formFieldProperty name="cd_consegnatario" />
	<formFieldProperty name="cognome_consegnatario" />
	<formFieldProperty name="cd_delegato" />
	<formFieldProperty name="cognome_delegato" />
	<formFieldProperty name="uo_consegnataria"/>
	<formFieldProperty name="delegato"/>
	<formFieldProperty name="consegnatario" />
	<formFieldProperty name="pg_inventario" />
	<formFieldProperty name="ti_documento" />
	<formFieldProperty name="esercizio" />
	<formFieldProperty name="pg_doc_trasporto_rientro" />
	<formFieldProperty name="ds_doc_trasporto_rientro" />
	<formFieldProperty name="data_registrazione" />
	<formFieldProperty name="cd_tipo_trasporto_rientro" />
	<formFieldProperty name="stato" />
	<formFieldProperty name="destinazione" />
	<formFieldProperty name="indirizzo" />

	<!--Campi ritiro delegato -->
	<formFieldProperty name="fl_incaricato" />
	<formFieldProperty name="fl_vettore" />
	<formFieldProperty name="find_dipendente" />
	<formFieldProperty name="cd_dipendente" />
	<formFieldProperty name="denominazione_dipendente" />
	<formFieldProperty name="uo_dipendente" />
	<formFieldProperty name="note_ritiro" />
	<formFieldProperty name="note" />

	<!-- ============================================ -->
	<!-- FIND FIELDS PROPERTY                         -->
	<!-- ============================================ -->

	<findFieldProperty name="pg_inventario" />
	<findFieldProperty name="ti_documento" />
	<findFieldProperty name="esercizio" />
	<findFieldProperty name="pg_doc_trasporto_rientro" />
	<findFieldProperty name="ds_doc_trasporto_rientro" />
	<findFieldProperty name="data_registrazione" />
	<findFieldProperty name="cd_tipo_trasporto_rientro" />
	<findFieldProperty name="tipoMovimento" />
	<findFieldProperty name="stato" />
	<findFieldProperty name="destinazione" />
	<findFieldProperty name="indirizzo" />
	<findFieldProperty name="fl_incaricato" />
	<findFieldProperty name="fl_vettore" />
	<findFieldProperty name="note_ritiro" />
	<findFieldProperty name="note" />
	<findFieldProperty name="nr_inventario" />
	<findFieldProperty name="cd_barre" />

	<!-- ============================================ -->
	<!-- COLUMN FIELDS PROPERTY                       -->
	<!-- ============================================ -->

	<columnFieldProperty name="pg_inventario" />
	<columnFieldProperty name="ti_documento" />
	<columnFieldProperty name="esercizio" />
	<columnFieldProperty name="pg_doc_trasporto_rientro" />
	<columnFieldProperty name="ds_doc_trasporto_rientro" />
	<columnFieldProperty name="data_registrazione" />
	<columnFieldProperty name="cd_tipo_trasporto_rientro" />
	<columnFieldProperty name="tipoMovimento" />
	<columnFieldProperty name="stato" />
	<columnFieldProperty name="destinazione" />
	<columnFieldProperty name="indirizzo" />

	<!-- ============================================ -->
	<!-- COLUMN SETS                                  -->
	<!-- ============================================ -->

	<columnSet name="TRASPORTO">
		<columnFieldProperty name="pg_inventario" />
		<columnFieldProperty name="esercizio" />
		<columnFieldProperty name="tipoMovimento" />
		<columnFieldProperty name="data_registrazione" label="Data Trasporto"/>
		<columnFieldProperty name="ds_doc_trasporto_rientro" />
		<columnFieldProperty name="pg_doc_trasporto_rientro" />
		<columnFieldProperty name="stato" />
		<columnFieldProperty name="destinazione" />
	</columnSet>

	<columnSet name="RIENTRO">
		<columnFieldProperty name="pg_inventario" />
		<columnFieldProperty name="esercizio" />
		<columnFieldProperty name="tipoMovimento" />
		<columnFieldProperty name="data_registrazione" label="Data Rientro"/>
		<columnFieldProperty name="ds_doc_trasporto_rientro" />
		<columnFieldProperty name="pg_doc_trasporto_rientro" />
		<columnFieldProperty name="stato" />
		<columnFieldProperty name="destinazione" />
	</columnSet>

	<!-- ============================================ -->
	<!-- FREE SEARCH SETS                             -->
	<!-- ============================================ -->

	<freeSearchSet name="TRASPORTO">
		<findFieldProperty name="tipoMovimento" />
		<findFieldProperty name="data_registrazione"/>
		<findFieldProperty name="ds_doc_trasporto_rientro" />
		<findFieldProperty name="pg_doc_trasporto_rientro" />
		<findFieldProperty name="stato" />
		<findFieldProperty name="destinazione" />
		<findFieldProperty name="indirizzo" />
		<findFieldProperty name="nr_inventario" />
		<findFieldProperty name="cd_barre" />
		<findFieldProperty name="fl_incaricato" />
		<findFieldProperty name="fl_vettore" />
	</freeSearchSet>

	<freeSearchSet name="RIENTRO">
		<findFieldProperty name="tipoMovimento" />
		<findFieldProperty name="data_registrazione"/>
		<findFieldProperty name="pg_doc_trasporto_rientro" />
		<findFieldProperty name="ds_doc_trasporto_rientro" />
		<findFieldProperty name="stato" />
		<findFieldProperty name="destinazione" />
		<findFieldProperty name="nr_inventario" />
		<findFieldProperty name="cd_barre" />
		<findFieldProperty name="fl_incaricato" />
		<findFieldProperty name="fl_vettore" />
	</freeSearchSet>

</bulkInfo>