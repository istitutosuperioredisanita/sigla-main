<?xml version="1.0" encoding="UTF-8"?>
<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA"
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">


	<fieldProperty
			name="pg_inventario"
			property="documentoTrasporto.inventario.pg_inventario"
			inputType="TEXT"
			inputSize="10"
			maxLength="10"
			label="Codice Inventario" />

	<fieldProperty
			name="ti_documento"
			property="documentoTrasporto.tiDocumento"
			inputType="TEXT"
			inputSize="1"
			maxLength="1"
			label="Tipo Documento" />

	<fieldProperty
			name="esercizio"
			property="documentoTrasporto.esercizio"
			inputType="TEXT"
			inputSize="4"
			maxLength="4"
			label="Esercizio" />

	<fieldProperty
			name="pg_doc_trasporto_rientro"
			property="documentoTrasporto.pgDocTrasportoRientro"
			inputType="TEXT"
			inputSize="10"
			maxLength="10"
			label="Numero Documento" />


	<fieldProperty
			name="esercizio_rif"
			property="docRientroDettRif.documentoRientro.esercizio"
			inputType="TEXT"
			inputSize="4"
			maxLength="4"
			label="Esercizio Rif" />

	<fieldProperty
			name="pg_inventario_rif"
			property="docRientroDettRif.documentoRientro.inventario.pg_inventario"
			inputType="TEXT"
			inputSize="10"
			maxLength="10"
			label="Codice Inventario Rif" />

	<fieldProperty
			name="ti_documento_rif"
			property="docRientroDettRif.documentoRientro.tiDocumento"
			inputType="TEXT"
			inputSize="1"
			maxLength="1"
			label="Tipo Documento Rif" />

	<fieldProperty
			name="pg_doc_trasporto_rientro_rif"
			property="docRientroDettRif.documentoRientro.pgDocTrasportoRientro"
			inputType="TEXT"
			inputSize="10"
			maxLength="10"
			label="Numero Documento Rif" />

	<fieldProperty
			name="progressivo_rif"
			property="docRientroDettRif.bene.progressivo"
			inputType="TEXT"
			inputSize="10"
			maxLength="10"
			label="pregressivo Rif" />

	<fieldProperty
			name="nr_inventario_rif"
			property="docRientroDettRif.bene.nr_inventario"
			inputType="TEXT"
			inputSize="10"
			maxLength="10"
			label="Numero Inventario Rif" />

	<columnSet name="righeTrasporto">
		<columnFieldProperty
				name="nr_inventario"
				style="width:80px"/>
		<columnFieldProperty
				name="etichetta"
				style="width:80px"/>
		<columnFieldProperty
				name="progressivo"/>
		<columnFieldProperty
				name="ds_bene"
				style="width:300px"/>
		<columnFieldProperty
				name="valore_bene"
				style="width:80px"/>
	</columnSet>

</bulkInfo>