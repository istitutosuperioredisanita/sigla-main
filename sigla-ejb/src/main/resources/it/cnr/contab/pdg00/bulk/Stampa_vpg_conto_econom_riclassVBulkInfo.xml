<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA"
		longDescription="Conto Economico Riclassificato"
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">

	<form
		name="/cnrpreventivo/pdg/contoeconomicoriclassificato.rpt">
		<formFieldProperty 
			name="esercizio"
			inputType="ROTEXT" />
		<formFieldProperty 
			name="cdCDSForPrint"/>
		<formFieldProperty 
			name="cdUOForPrint" />
	</form>

	<printForm
		name="/cnrpreventivo/pdg/contoeconomicoriclassificato.rpt">
		<printFieldProperty 
			name="tc"
			parameterPosition="0"
			parameterType="STRING" />
		<printFieldProperty 
			name="ti_ist_com"
			parameterPosition="1"
			parameterType="STRING" />
		<printFieldProperty 
			name="esercizio"
			parameterPosition="2"
			parameterType="STRING" />
		<printFieldProperty 
			name="cdCDSCRForPrint"
			parameterPosition="3"
			parameterType="STRING" />
		<printFieldProperty 
			name="cdUOCRForPrint"
			parameterPosition="4"
			parameterType="STRING" />	
	</printForm>

	<form
		name="/cnrpreventivo/pdg/contoeconomicoriclassificato.jasper">
		<formFieldProperty 
			name="esercizio"
			inputType="ROTEXT" />
		<formFieldProperty 
			name="cdCDSForPrint"/>
		<formFieldProperty 
			name="cdUOForPrint" />
	</form>

	<printForm
		name="/cnrpreventivo/pdg/contoeconomicoriclassificato.jasper">
		<printFieldProperty 
			name="ti_ist_com"
			parameterPosition="1"
			parameterType="STRING"
			paramNameJR="IST_COMM"
			paramTypeJR="java.lang.String" />
		<printFieldProperty 
			name="esercizio"
			parameterPosition="2"
			parameterType="STRING"
			paramNameJR="inEs"
			paramTypeJR="java.lang.Integer" />
		<printFieldProperty 
			name="cdCDSCRForPrint"
			parameterPosition="3"
			parameterType="STRING"
			paramNameJR="CDS"
			paramTypeJR="java.lang.String" />
		<printFieldProperty 
			name="cdUOCRForPrint"
			parameterPosition="4"
			parameterType="STRING"
			paramNameJR="uo"
			paramTypeJR="java.lang.String" />	
		<printFieldProperty 
			name="dettaglioContiYN"
			parameterPosition="5"
			parameterType="STRING"
			paramNameJR="dettaglioConti"
			paramTypeJR="java.lang.String" />
		<printFieldProperty
			name="cdTipoBilancio"
			parameterPosition="6"
			parameterType="STRING"
			paramNameJR="CD_TIPO_BILANCIO"
			paramTypeJR="java.lang.String" />

	</printForm>		
	
</bulkInfo>