<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA"
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">

	<fieldProperty
			name="esercizio"
			property="esercizio"
			inputType="TEXT"
			inputSize="22"
			maxLength="22"
			label="Esercizio" />
	<fieldProperty
			name="esercizio_res"
			property="esercizio_res"
			inputType="TEXT"
			inputSize="22"
			maxLength="22"
			label="Esercizio Ori." />
	<fieldProperty
			name="cd_centro_responsabilita"
			property="cd_centro_responsabilita"
			inputType="TEXT"
			inputSize="30"
			maxLength="30"
			label="Cd CDR" />
	<fieldProperty
			name="ti_appartenenza"
			property="ti_appartenenza"
			inputType="TEXT"
			inputSize="5"
			maxLength="1"
			label="Tipo Appartenenza" />
	<fieldProperty
			name="ti_gestione"
			property="ti_gestione"
			inputType="TEXT"
			inputSize="5"
			maxLength="1"
			label="Tipo Gestione" />
	<fieldProperty
			name="cd_elemento_voce"
			property="cd_elemento_voce"
			inputType="TEXT"
			inputSize="20"
			maxLength="20"
			label="Cd Elemento Voce" />
	<fieldProperty
			name="descrizione_voce"
			property="descrizione_voce"
			inputType="TEXT"
			inputSize="60"
			maxLength="300"
			label="Descrizione Voce"
			enabledOnSearch="true" />
	<fieldProperty
			name="cds_obb"
			property="cds_obb"
			inputType="TEXT"
			inputSize="30"
			maxLength="30"
			label="CdS Obb." />
	<fieldProperty
			name="pg_obb"
			property="pg_obb"
			inputType="TEXT"
			inputSize="10"
			maxLength="10"
			label="Prog. Obb." />
	<fieldProperty
			name="ds_obbligazione"
			property="ds_obbligazione"
			inputType="TEXT"
			inputSize="60"
			maxLength="300"
			label="Desc. Obb."
			enabledOnSearch="true" />
	<fieldProperty
			name="pg_modific"
			property="pg_modific"
			inputType="TEXT"
			inputSize="10"
			maxLength="10"
			label="Prog. Modifica" />
	<fieldProperty
			name="cd_tipo_documento_cont"
			property="cd_tipo_documento_cont"
			inputType="TEXT"
			inputSize="10"
			maxLength="10"
			label="Tipo Documento Cont" />
	<fieldProperty
			name="descrModifica"
			property="descrModifica"
			inputType="TEXT"
			inputSize="60"
			maxLength="300"
			label="Desc. Modifica" />
	<fieldProperty
			name="var_piu_obb_res_pro"
			property="var_piu_obb_res_pro"
			inputType="TEXT"
			formatName="it.cnr.contab.util.EuroFormat"
			maxLength="16"
			inputSize="16"
			label="Var + Obb. Res." />
	<fieldProperty
			name="var_meno_obb_res_pro"
			property="var_meno_obb_res_pro"
			inputType="TEXT"
			formatName="it.cnr.contab.util.EuroFormat"
			maxLength="16"
			inputSize="16"
			label="Var - Obb. Res." />
	<fieldProperty
			name="im_improprio"
			property="im_improprio"
			inputType="TEXT"
			formatName="it.cnr.contab.util.EuroFormat"
			maxLength="16"
			inputSize="16"
			label="Imp. Improprio" />

	<!-- Form Fields -->
	<formFieldProperty name="esercizio" />
	<formFieldProperty name="esercizio_res" />
	<formFieldProperty name="cd_centro_responsabilita" />
	<formFieldProperty name="ti_appartenenza" />
	<formFieldProperty name="ti_gestione" />
	<formFieldProperty name="cd_elemento_voce" />
	<formFieldProperty name="descrizione_voce" />
	<formFieldProperty name="cds_obb" />
	<formFieldProperty name="pg_obb" />
	<formFieldProperty name="ds_obbligazione" />
	<formFieldProperty name="pg_modific" />
	<formFieldProperty name="cd_tipo_documento_cont" />
	<formFieldProperty name="descrModifica" />
	<formFieldProperty name="var_piu_obb_res_pro" />
	<formFieldProperty name="var_meno_obb_res_pro" />
	<formFieldProperty name="im_improprio" />

	<!-- Find Fields -->
	<findFieldProperty name="esercizio" />
	<findFieldProperty name="esercizio_res" />
	<findFieldProperty name="cd_centro_responsabilita" />
	<findFieldProperty name="ti_appartenenza" />
	<findFieldProperty name="ti_gestione" />
	<findFieldProperty name="cd_elemento_voce" />
	<findFieldProperty name="descrizione_voce" />
	<findFieldProperty name="cds_obb" />
	<findFieldProperty name="pg_obb" />
	<findFieldProperty name="ds_obbligazione" />
	<findFieldProperty name="pg_modific" />
	<findFieldProperty name="cd_tipo_documento_cont" />

	<!-- Column Fields - campi da visualizzare nella tabella -->
	<columnFieldProperty name="esercizio" />
	<columnFieldProperty name="esercizio_res" />
	<columnFieldProperty name="cd_centro_responsabilita" />
	<columnFieldProperty name="cd_elemento_voce" />
	<columnFieldProperty name="descrizione_voce" />
	<columnFieldProperty name="cds_obb" />
	<columnFieldProperty name="pg_obb" />
	<columnFieldProperty name="ds_obbligazione" />
	<columnFieldProperty name="pg_modific" />
	<columnFieldProperty name="cd_tipo_documento_cont" />
	<columnFieldProperty name="descrModifica" />
	<columnFieldProperty name="var_piu_obb_res_pro" />
	<columnFieldProperty name="var_meno_obb_res_pro" />
	<columnFieldProperty name="im_improprio" />

</bulkInfo>