            <column name="utcr" type="VARCHAR(20)" value="SYSTEM"/>
            <column name="pg_ver_rec" type="BIGINT" value="0"/>
        </insert>

        <!-- ========================================== -->
        <!-- STRUTTURA ALBERO MENU -->
        <!-- ========================================== -->

        <!-- Nodo: Tipo Trasporto/Rientro Inventario -->
        <insert tableName="albero_main">
            <column name="cd_nodo" type="VARCHAR(100)" value="0.AMM.INVENT.TABRIF.TIPOTRRI"/>
            <column name="ds_nodo" type="VARCHAR(200)" value="Tipo Trasporto/Rientro Inventario"/>
            <column name="pg_ordinamento" type="BIGINT" value="70"/>
            <column name="fl_terminale" type="CHAR(1)" value="N"/>
            <column name="livello" type="SMALLINT" value="4"/>
            <column name="cd_proprio_nodo" type="VARCHAR(100)" value="TIPOTRRI"/>
            <column name="cd_nodo_padre" type="VARCHAR(100)" value="0.AMM.INVENT.TABRIF"/>
            <column name="duva" type="${date.type}" valueDate="${now}"/>
            <column name="utuv" type="VARCHAR(20)" value="SYSTEM"/>
            <column name="dacr" type="${date.type}" valueDate="${now}"/>
            <column name="utcr" type="VARCHAR(20)" value="SYSTEM"/>
            <column name="pg_ver_rec" type="BIGINT" value="0"/>
        </insert>

        <!-- Voce menu: Tipo Trasporto Rientro - Visualizzazione -->
        <insert tableName="albero_main">
            <column name="cd_nodo" type="VARCHAR(100)" value="0.AMM.INVENT.TABRIF.TIPOTRRI.V"/>
