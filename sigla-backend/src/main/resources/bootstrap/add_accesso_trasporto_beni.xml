    <changeSet id="add_accesso_trasporto_beni" author="davide.mirra">

        <!-- Accesso per Visualizzazione -->
        <insert tableName="accesso">
            <column name="cd_accesso" type="VARCHAR(20)" value="AMMINVENTTRASBENIV"/>
